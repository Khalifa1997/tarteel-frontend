{
  "name": "Tarteel",
  "version": "2.3.1",
  "description": "Tarteel is an open-source project designed to help build digital tools to analyze the recitation of the Quran.",
  "main": "build/server/index.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "analyze:client": "cross-env NODE_ENV=production ts-node -O '{\"module\": \"commonjs\"}' --transpileOnly internal/scripts/analyze -- --client",
    "analyze:server": "ts-node -O '{\"module\": \"commonjs\"}' --transpileOnly internal/scripts/analyze -- --server",
    "build": "ts-node -T -O '{\"module\": \"commonjs\"}' --transpileOnly internal/scripts/build -- --optimize",
    "build:dev": "ts-node -O '{\"module\": \"commonjs\"}' --transpileOnly internal/scripts/build",
    "clean": "cross-env ts-node -O '{\"module\": \"commonjs\"}' --transpileOnly internal/scripts/clean",
    "deploy": "babel-node internal/scripts/deploy",
    "dev": "cross-env DEPLOYMENT=development ts-node -O '{\"module\": \"commonjs\"}' --transpileOnly internal/development",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "format:src": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "format:config": "prettier --write \"config/**/*.{js,jsx,ts,tsx}\"",
    "format:internal": "prettier --write \"internal/**/*.{js,jsx,ts,tsx}\"",
    "precommit": "lint-staged",
    "preinstall": "node internal/scripts/preinstall",
    "prepush": "jest",
    "start": "cross-env NODE_ENV=production node build/server",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx,ts,tsx}": [
        "prettier --write",
        "git add"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tarteel-io/tarteel-frontend.git"
  },
  "keywords": [
    "Tarteel",
    "Quran",
    "Voice Recognition"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Tarteel-io/tarteel-frontend/issues"
  },
